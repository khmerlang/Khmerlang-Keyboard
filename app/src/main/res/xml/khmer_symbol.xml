<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height">
    <Row android:keyHeight="@dimen/top_keys_height">
        <Key
            android:codes="06113"
            android:keyEdgeFlags="left"
            android:keyLabel="1"
            app:subLabel="q"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06114"
            android:keyLabel="2"
            app:subLabel="w"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06115"
            android:keyLabel="3"
            app:subLabel="e"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06116"
            android:keyLabel="4"
            app:subLabel="r"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06117"
            android:keyLabel="5"
            app:subLabel="t"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06118"
            android:keyLabel="6"
            app:subLabel="y"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06119"
            android:keyLabel="7"
            app:subLabel="u"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06120"
            android:keyLabel ="8"
            app:subLabel="i"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06121"
            android:keyLabel="9"
            app:subLabel="o"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06066"
            android:keyEdgeFlags="right"
            android:keyLabel="0"
            android:keyWidth="10%p"/>
    </Row>
    <Row android:keyHeight="@dimen/key_kh_height">
        <Key
            android:codes="06113"
            android:keyEdgeFlags="left"
            android:keyLabel="+"
            app:subLabel="q"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06114"
            android:keyLabel="×"
            app:subLabel="w"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06115"
            android:keyLabel="÷"
            app:subLabel="e"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06116"
            android:keyLabel="="
            app:subLabel="r"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06117"
            android:keyLabel="/"
            app:subLabel="t"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06118"
            android:keyLabel="_"
            app:subLabel="y"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06119"
            android:keyLabel="៛"
            app:subLabel="u"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06120"
            android:keyLabel ="€"
            app:subLabel="i"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06121"
            android:keyLabel="£"
            app:subLabel="o"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06066"
            android:keyEdgeFlags="right"
            android:keyLabel="¥"
            android:keyWidth="10%p"/>
    </Row>

    <Row android:keyHeight="@dimen/key_kh_height">
        <Key
            android:codes="06113"
            android:keyEdgeFlags="left"
            android:keyLabel="!"
            app:subLabel="q"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06114"
            android:keyLabel="\@"
            app:subLabel="w"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06115"
            android:keyLabel="\#"
            app:subLabel="e"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06116"
            android:keyLabel="$"
            app:subLabel="r"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06117"
            android:keyLabel="%"
            app:subLabel="t"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06118"
            android:keyLabel="^"
            app:subLabel="y"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06119"
            android:keyLabel="&amp;"
            app:subLabel="u"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06120"
            android:keyLabel ="*"
            app:subLabel="i"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06121"
            android:keyLabel="("
            app:subLabel="o"
            android:keyWidth="10%p"/>
        <Key
            android:codes="06066"
            android:keyEdgeFlags="right"
            android:keyLabel=")"
            android:keyWidth="10%p"/>
    </Row>

    <Row android:keyHeight="@dimen/key_kh_height">
        <Key
            android:codes="@integer/keycode_sym_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyLabel="1/2"
            android:keyWidth="15%p" />

        <Key
            android:codes="06105"
            android:keyLabel="៙" />
        <Key
            android:codes="06106"
            android:keyLabel="៚" />
        <Key
            android:codes="06108"
            android:keyLabel="ៜ" />
        <Key
            android:codes="06109"
            android:keyLabel="៝" />
        <Key
            android:codes="06107"
            android:keyLabel=";" />
        <Key
            android:codes="00036"
            android:keyLabel="[" />
        <Key
            android:codes="00064"
            android:keyLabel="]" />
        <Key
            android:codes="@integer/keycode_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace_fill"
            android:keyWidth="15%p" />
    </Row>

    <Row
        android:keyHeight="@dimen/bottom_keys_height"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/keycode_abc"
            android:keyEdgeFlags="left"
            android:keyLabel="កខគ"
            android:keyWidth="15%p"
            app:keyTextSize="6dp" />
        <Key
            android:codes="@integer/keycode_switch_next_keyboard"
            android:keyIcon="@drawable/ic_baseline_swap_horiz"
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:keyWidth="40%p"
            android:keyLabel="@string/app_name_kh"
            app:isChangeLanguageKey="true"
            app:keyTextSize="6dp" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_done"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>