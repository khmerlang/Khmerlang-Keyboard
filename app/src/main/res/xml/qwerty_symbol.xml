<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height">

    <Row>
        <Key
            android:codes="49"
            android:keyEdgeFlags="left"
            android:keyLabel="1"
            app:subLabel="១"/>
        <Key
            android:codes="50"
            android:keyLabel="2"
            app:subLabel="២"/>
        <Key
            android:codes="51"
            android:keyLabel="3"
            app:subLabel="៣"/>
        <Key
            android:codes="52"
            android:keyLabel="4"
            app:subLabel="៤"/>
        <Key
            android:codes="53"
            android:keyLabel="5"
            app:subLabel="៥"/>
        <Key
            android:codes="54"
            android:keyLabel="6"
            app:subLabel="៦"/>
        <Key
            android:codes="55"
            android:keyLabel="7"
            app:subLabel="៧"/>
        <Key
            android:codes="56"
            android:keyLabel="8"
            app:subLabel="៨"/>
        <Key
            android:codes="57"
            android:keyLabel="9"
            app:subLabel="៩"/>
        <Key
            android:codes="48"
            android:keyEdgeFlags="right"
            android:keyLabel="0"
            app:subLabel="០"/>
    </Row>

    <Row>
        <Key
            android:keyEdgeFlags="left"
            android:codes="64"
            android:keyLabel="\@"
            app:subLabel="ៗ"/>
        <Key
            android:codes="35"
            android:keyLabel="\#"
            app:subLabel="៛"/>
        <Key
            android:codes="36"
            android:keyLabel="$"
            app:subLabel="៍"/>
        <Key
            android:codes="37"
            android:keyLabel="%"
            app:subLabel="័"/>
        <Key
            android:codes="38"
            android:keyLabel="&amp;"
            app:subLabel="៏"/>
        <Key
            android:codes="42"
            android:keyLabel="*"
            app:subLabel="៌"/>
        <Key
            android:codes="45"
            android:keyLabel="-"
            app:subLabel="់"/>
        <Key
            android:codes="61"
            android:keyLabel="="
            app:subLabel="៉"/>
        <Key
            android:codes="40"
            android:keyLabel="("
            app:subLabel="៖"/>
        <Key
            android:codes="41"
            android:keyEdgeFlags="right"
            android:keyLabel=")"
            app:subLabel="ឧ"/>
    </Row>

    <Row>
        <Key
            android:codes="@integer/keycode_sym_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyLabel="=\\&gt;"
            android:keyWidth="15%p" />

        <Key
            android:codes="33"
            android:keyLabel="!"
            app:subLabel="ឥ"/>
        <Key
            android:codes="34"
            android:keyLabel="&quot;"
            app:subLabel="ឲ"/>
        <Key
            android:codes="39"
            android:keyLabel="\'"
            app:subLabel="ឰ"/>
        <Key
            android:codes="58"
            android:keyLabel=":"
            app:subLabel="ឯ"/>
        <Key
            android:codes="59"
            android:keyLabel=";"
            app:subLabel="ៀ"/>
        <Key
            android:codes="47"
            android:keyLabel="/"
            app:subLabel="ឿ"/>
        <Key
            android:codes="63"
            android:keyLabel="\?"
            app:subLabel="ឪ"/>
        <Key
            android:codes="@integer/keycode_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace"
            android:keyWidth="15%p" />
    </Row>

    <Row
        android:keyHeight="@dimen/bottom_keys_height"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/keycode_abc"
            android:keyEdgeFlags="left"
            android:keyLabel="ABC"
            android:keyWidth="15%p"
            app:keyTextSize="6dp" />
        <Key
            android:codes="@integer/keycode_number_shift"
            android:keyIcon="@drawable/ic_dialpad"
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:keyWidth="40%p"
            app:isChangeLanguageKey="true"
            android:keyLabel="@string/app_name"
            app:keyTextSize="6dp" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_done"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>