<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height"
    android:verticalGap="0px">

    <Row>
        <Key
            android:codes="81"
            android:keyEdgeFlags="left"
            android:keyLabel="Q"
            app:subLabel="ឈ"/>
        <Key
            android:codes="87"
            android:keyLabel="W"
            app:subLabel="ឺ"/>
        <Key
            android:codes="69"
            android:keyLabel="E"
            app:subLabel="ែ"/>
        <Key
            android:codes="82"
            android:keyLabel="R"
            app:subLabel="ឬ"/>
        <Key
            android:codes="84"
            android:keyLabel="T"
            app:subLabel="ទ"/>
        <Key
            android:codes="89"
            android:keyLabel="Y"
            app:subLabel="ួ"/>
        <Key
            android:codes="85"
            android:keyLabel="U"
            app:subLabel="ូ"/>
        <Key
            android:codes="73"
            android:keyLabel="I"
            app:subLabel="ី"/>
        <Key
            android:codes="79"
            android:keyLabel="O"
            app:subLabel="ៅ"/>
        <Key
            android:codes="80"
            android:keyEdgeFlags="right"
            android:keyLabel="P"
            app:subLabel="ភ"/>
    </Row>

    <Row>
        <Key
            android:codes="65"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="A"
            app:subLabel="ឫ"/>
        <Key
            android:codes="83"
            android:keyLabel="S"
            app:subLabel="ៃ"/>
        <Key
            android:codes="68"
            android:keyLabel="D"
            app:subLabel="ឌ"/>
        <Key
            android:codes="70"
            android:keyLabel="F"
            app:subLabel="ធ"/>
        <Key
            android:codes="71"
            android:keyLabel="G"
            app:subLabel="អ"/>
        <Key
            android:codes="72"
            android:keyLabel="H"
            app:subLabel="ះ"/>
        <Key
            android:codes="74"
            android:keyLabel="J"
            app:subLabel="ញ"/>
        <Key
            android:codes="75"
            android:keyLabel="K"
            app:subLabel="គ"/>
        <Key
            android:codes="76"
            android:keyEdgeFlags="right"
            android:keyLabel="L"
            app:subLabel="ឡ"/>
    </Row>

    <Row>
        <Key
            android:codes="@integer/keycode_unshift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_keyboard_capslock"
            android:keyWidth="15%p" />
        <Key
            android:codes="90"
            android:keyLabel="Z"
            app:subLabel="ឍ"/>
        <Key
            android:codes="88"
            android:keyLabel="X"
            app:subLabel="ឃ"/>
        <Key
            android:codes="67"
            android:keyLabel="C"
            app:subLabel="ជ"/>
        <Key
            android:codes="86"
            android:keyLabel="V"
            app:subLabel="ៈ"/>
        <Key
            android:codes="66"
            android:keyLabel="B"
            app:subLabel="ព"/>
        <Key
            android:codes="78"
            android:keyLabel="N"
            app:subLabel="ណ"/>
        <Key
            android:codes="77"
            android:keyLabel="M"
            app:subLabel="ំ"/>
        <Key
            android:codes="@integer/keycode_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace"
            android:keyWidth="15%p" />
    </Row>

    <Row
        android:keyHeight="@dimen/bottom_keys_height"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/keycode_sym"
            android:keyEdgeFlags="left"
            android:keyLabel="\?123"
            android:keyWidth="15%p"
            app:keyTextSize="6dp" />
        <Key
            android:codes="@integer/keycode_switch_next_keyboard"
            android:keyIcon="@drawable/ic_language"
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:keyWidth="40%p"
            app:isChangeLanguageKey="true"
            app:keyTextSize="6dp"
            android:keyLabel="@string/app_name"/>
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_done"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>