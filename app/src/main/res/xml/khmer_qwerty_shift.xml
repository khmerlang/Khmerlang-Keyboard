<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height"
    android:verticalGap="0px">
    <Row android:keyHeight="@dimen/key_kh_height">
        <Key
            android:codes="06076"
            android:keyLabel="ូ"/>
        <Key
            android:codes="06073"
            android:keyLabel="ឹ"/>
        <Key
            android:codes="06074"
            android:keyLabel="ឺ"/>
        <Key
            android:codes="6066"
            android:keyLabel="ឲ"/>
        <Key
            android:codes="06022"
            android:keyLabel="ឆ"/>
        <Key
            android:codes="06019"
            android:keyLabel="ឃ"/>
        <Key
            android:codes="06044"
            android:keyLabel="វ"/>
        <Key
            android:codes="06028"
            android:keyLabel="ឌ"/>
        <Key
            android:codes="06039"
            android:keyLabel="ភ"/>
        <Key
            android:codes="06037"
            android:keyLabel="ផ"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key
            android:keyEdgeFlags="left"
            android:codes="06077"
            android:keyLabel="ួ"/>
        <Key
            android:codes="6080"
            android:keyLabel="ៀ"/>
        <Key
            android:codes="06084"
            android:keyLabel="ោ"/>
        <Key
            android:codes="06050"
            android:keyLabel="អ"/>
        <Key
            android:codes="06027"
            android:keyLabel="ឋ"/>
        <Key
            android:codes="06034"
            android:keyLabel="ធ"/>
        <Key
            android:codes="06032"
            android:keyLabel="ថ"/>
        <Key
            android:codes="06048"
            android:keyLabel="ហ"/>
        <Key
            android:codes="06018"
            android:keyLabel="គ"/>
        <Key
            android:codes="06049"
            android:keyLabel="ឡ"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key
            android:codes="@integer/keycode_unshift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_keyboard_capslock"
            android:keyWidth="15%p" />
        <Key
            android:codes="6079"
            android:keyLabel="ឿ"/>
        <Key
            android:codes="06085"
            android:keyLabel="ៅ"/>
        <Key
            android:codes="06087"
            android:keyLabel="ះ"/>
        <Key
            android:codes="06029"
            android:keyLabel="ឍ"/>
        <Key
            android:codes="06017"
            android:keyLabel="ខ"/>
        <Key
            android:codes="06030"
            android:keyLabel="ណ"/>
        <Key
            android:codes="06024"
            android:keyLabel="ឈ"/>
        <Key
            android:codes="@integer/keycode_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace_fill"
            android:keyWidth="15%p" />
    </Row>

    <Row
        android:keyHeight="@dimen/bottom_keys_height"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/keycode_sym"
            android:keyEdgeFlags="left"
            android:keyLabel="១២៣"
            android:keyWidth="15%p"
            app:keyTextSize="6dp" />
        <Key
            android:codes="@integer/keycode_mode_change"
            android:keyIcon="@drawable/ic_baseline_swap_horiz"
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_switch_next_keyboard"
            android:keyIcon="@drawable/ic_language"
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:keyWidth="40%p"
            app:keyTextSize="6dp"
            android:keyLabel="@string/app_name_on_key_kh"
            app:isChangeLanguageKey="true"/>
        <Key
            android:codes="00063"
            android:keyLabel="\?"
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_done"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>