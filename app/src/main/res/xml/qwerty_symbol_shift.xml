<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height">

    <Row>
        <Key
            android:keyEdgeFlags="left"
            android:codes="126"
            android:keyLabel="~" />
        <Key
            android:codes="177"
            android:keyLabel="±" />
        <Key
            android:codes="215"
            android:keyLabel="×" />
